<div class="quizapp-tools player-block object-layouts-container-layout-2 " ng:if="__player_object.settings.time_settings.is_with_time == true || __player_object.settings.progression_bar.is_available == true">
<style>
      .object-layouts-container-layout-2 {
          width: 63%;
          height: auto;
          font-size: 15px;
          position: relative;
          /* overflow: hidden; */
          /*background: #f9f9f9;*/
          margin: 15px auto 0px auto;
          line-height: 2;
          /*box-shadow: 0px 0px 2px 0px #e9e9e9;
          border-bottom: 1px dashed #999;
          border: 1px solid #ddd;
          border-radius: 15px;*/
       }
       .base-circle {
         fill: none;
         stroke: #eee;
         stroke-width: 5px;
        }
        .current-progress {
          fill: none;
          stroke: #c17c7e;
          stroke-width: 15px;
          transition: stroke-dashoffset 0.7s;
          stroke-dasharray: 628 ;
          stroke-dashoffset: 628 ; /* 800 */
          animation: dash 5s linear forwards;
        }
        /*@keyframes dash {
          to {
            stroke-dashoffset: 0;
          }
        }*/
        .time-data-hrs {
          font-size: 60px;
          font-family: sans-serif;
        }
        .time-data {
          font-size: 35px;
          font-family: "diogene_carre";
        }
        .time-data-type {
          font-size: 25px;
          font-family: sans-serif;
        }
</style>

    <div class="circle text-center" ng:init="all_seconds_act = __player_object.settings.time_settings.value / 60 ">

      <svg width="120" viewBox="0 0 220 220" xmlns="http://www.w3.org/2000/svg">
         <g transform="translate(110,110)">
            <circle r="100" class="base-circle"/>
            <g transform="rotate(-90)">
               <circle r="100" id="current-progress" class="current-progress"/>
            </g>

            <!-- <text ng:if=" ( (all_seconds_act | math_around_it) | length_it ) >= 3" transform="translate(-50,0)" class="time-data hr"></text>
            <text ng:if=" ( (all_seconds_act | math_around_it) | length_it ) >= 3" transform="translate(25,0)" class="time-data min"></text>

            <text ng:if=" ( (all_seconds_act | math_around_it) | length_it ) < 3" transform="translate(-45,0)" class="time-data min"></text>
            <text ng:if=" ( (all_seconds_act | math_around_it) | length_it ) < 3" transform="translate(8,0)" class="time-data sec"></text> -->
           <text transform="translate(-30,30)" class="time-data-type">mins</text>
         </g>
      </svg>
    </div>
</div>
