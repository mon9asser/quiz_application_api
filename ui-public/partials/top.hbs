<!DOCTYPE html>
<html ng:app="applications" ng:controller="page-controller">
  <head>
    <meta charset="utf-8">
    <title></title>

    <!--
      ++++++++++++++++++++++++++++++++
       ++ Basics js frameworks
      ++++++++++++++++++++++++++++++++
    -->
    <!-- JS -->
    <script src="{{ server_ip }}ext/frameworks/js/jquery.min.js" charset="utf-8"></script>
    <script src="{{ server_ip }}ext/frameworks/js/angular.min.js" charset="utf-8"></script>
    <!-- Css -->
    <link href="{{ server_ip }}ext/frameworks/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ server_ip }}ext/frameworks/css/font-awesome.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="{{ server_ip }}ext/frameworks/js/html5shiv.min.js"></script>
      <script src="{{ server_ip }}ext/frameworks/js/respond.min.js"></script>
    <![endif]-->

    <!--
      ++++++++++++++++++++++++++++++++
       ++ Styles
      +++++++++ng:app="applications" ng:controller="page-controller"+++++++++page_name++++++++++++++
    -->
    <!-- Login css -->
    <link ng:if="page_name=='login'" href="{{ server_ip }}ext/css/login.css" rel="stylesheet">
    <!-- Register Css -->
    <link ng:if="page_name=='register'" href="{{ server_ip }}ext/css/register.css" rel="stylesheet">
    <!-- My questionnaires -->
    <link ng:if="page_name=='questionnaires'" href="{{ server_ip }}ext/css/questionnaire.css" rel="stylesheet">
    <!-- Edit applications -->
    <link ng:if="page_name=='api'" href="{{ server_ip }}ext/css/apps.css" rel="stylesheet">


  </head>
  <body >
    <!-- Required Values -->
    <input id="userId" type="hidden" name="" value="{{user.id}}">
    <input id="userEmail" type="hidden" name="" value="{{user.email}}">
    <input id="userName" type="hidden" name="" value="{{user.name}}">
    <input id="userToken" type="hidden" name="" value="{{user.token}}">
    <input id="serverIp" type="hidden" name="" value="{{ server_ip }}">
    <input id="applicationId" type="hidden" name="" value="{{app._id}}">
