    <div>
        <div class="tick-group"  ng:if="_settings_.time_settings.timer_type == true">
            <span class="hr">{>__player_object.settings.time_settings.hours<}</span>
            <sub>h</sub>
        </div>
        <div class="tick-group">
            <span class="tick-text min">{>__player_object.settings.time_settings.minutes <}</span>
            <sub>m</sub>
        </div>
        <div class="tick-group">
            <span class="tick-text sec">{>__player_object.settings.time_settings.seconds <}</span>
            <sub> s</sub>
        </div>
    </div>
